{
  "name": "kitchen-tools",
  "version": "0.2.0",
  "description": "Tool to manage monorepo with service oriented architecture based on docker and docker-compose",
  "bin": {
    "bake": "./src/bake.js",
    "oven": "./src/oven.js",
    "bake-mix": "./src/mix.js",
    "bake-help": "./src/help.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -p tsconfig.json --watch",
    "test": "yarn run build && jasmine src/**/*_spec.js",
    "schematics": "schematics",
    "np": "np --no-2fa"
  },
  "keywords": [
    "kitchen-tools",
    "monorepo",
    "docker",
    "docker-compose"
  ],
  "author": "Jacopo Martinelli <jacopo.patroclo.martinelli@gmail.com> (https://github.com/JacopoPatroclo)",
  "license": "MIT",
  "schematics": "./src/collection.json",
  "dependencies": {
    "@angular-devkit/core": "^10.0.4",
    "@angular-devkit/schematics": "^10.0.4",
    "@angular-devkit/schematics-cli": "^0.1000.4",
    "js-yaml": "^3.14.0",
    "lodash": "^4.17.19",
    "marked": "^1.1.1",
    "marked-terminal": "^4.1.0",
    "typescript": "~3.9.7"
  },
  "devDependencies": {
    "@types/jasmine": "~3.5.0",
    "@types/js-yaml": "^3.12.5",
    "@types/lodash": "^4.14.158",
    "@types/node": "^12.11.1",
    "jasmine": "^3.5.0",
    "np": "^6.3.2"
  }
}

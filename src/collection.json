{
  "$schema": "../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "init": {
      "description": "Init the project",
      "factory": "./schematics/init/index#init",
      "schema": "./schematics/init/schema.json"
    },
    "new": {
      "description": "Create a new service",
      "factory": "./schematics/new/index#nev",
      "schema": "./schematics/new/schema.json"
    },
    "custom": {
      "description": "Create a custom service",
      "factory": "./schematics/custom/index#custom",
      "schema": "./schematics/custom/schema.json"
    },
    "service": {
      "description": "Create an internal new service",
      "factory": "./schematics/service/index#service",
      "schema": "./schematics/service/schema.json"
    }
  }
}
